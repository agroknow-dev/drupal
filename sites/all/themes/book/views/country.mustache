{{>header}}
<input type="hidden" id="entity-id" value="{{entity-id}}" />
<div class="content main-content container">
	<ol class="breadcrumb">
	  <li><a href="/regions/global">{{#labels}}{{land_book}}{{/labels}}</a></li>
	  <li><a href="/countries">{{#labels}}{{countries}}{{/labels}}</a></li>
	  <li><a id="continent-link" href="">{{#info}}{{continent}}{{/info}}</a></li>
	  <li class="active"><span class="country-name">{{#info}}{{country}}{{/info}}</span></li>
	</ol>
	<div class="row">
		<div class="col-sm-12">
			<h1 class="country-name"><span class="country-name"></span></h1>
		</div>
	</div>
	<div class="row">
	  <div class="col-sm-3">
		  <iframe class="map" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl={{#application}}{{language}}{{/application}}&amp;geocode=&amp;q={{#info}}{{country}}{{/info}}&amp;aq=&amp;ie=UTF8&amp;hq=&amp;hnear={{#info}}{{country}}{{/info}}&amp;t=m&amp;z=4&amp;output=embed"></iframe>
		  <h2 class="section"><span>{{#labels}}{{main_index_rankings}}{{/labels}}</span></h2>
		  <div id="main-index-rankings" class="graph">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#main-index-rankings}}
	                    {{>table}}
	                {{/main-index-rankings}}
            	</div>
            {{/charts}}
          </div>
		  <h2 class="section"><span>{{#labels}}{{gender_issues}}{{/labels}}</span></h2>
		  <table>
			<tr>
				<td class="issue">
					Inheritance rights of spouses
				</td>
				<td>
					<div class="circle"><div class="green"></div></div>
				</td>
			</tr>
			<tr>
				<td class="issue">
					Inheritance rights of daughters
				</td>
				<td>
					<div class="circle"><div class="red"></div></div>
				</td>
			</tr>
			<tr>
				<td class="issue">
					Access to land
				</td>
				<td>
					<div class="circle"><div class="yellow"></div></div>
				</td>
			</tr>
		  </table>
		  <h2 class="section"><span>{{#labels}}{{rural_development}}{{/labels}}</span></h2>
		  <div class="row">
			<div class="col-xs-4">
			  <div id="rural-development-1" class="graph-small text-center">
	            {{#charts}}
	            	<div class="source-graph chart-content">
		                {{#rural-development-1}}
		                    {{>table}}
		                {{/rural-development-1}}
	            	</div>
	            {{/charts}}
	            HDI
	          </div>
			</div>
			<div class="col-xs-4">
			  <div id="rural-development-2" class="graph-small text-center">
	            {{#charts}}
	            	<div class="source-graph chart-content">
		                {{#rural-development-2}}
		                    {{>table}}
		                {{/rural-development-2}}
	            	</div>
	            {{/charts}}
	            GHI
	          </div>
			</div>
			<div class="col-xs-4">
			  <div id="rural-development-3" class="graph-small text-center">
	            {{#charts}}
	            	<div class="source-graph chart-content">
		                {{#rural-development-3}}
		                    {{>table}}
		                {{/rural-development-3}}
	            	</div>
	            {{/charts}}
	            Population
	          </div>
			</div>
		  </div>
	  </div>
	  <div class="col-sm-6 graph-section">
		  <h2 class="section"><span>{{#labels}}{{socio_economic_values}}{{/labels}}</span></h2>
          <div class="socioeconomic-values chart-content">
            {{#charts}}
                {{#socioeconomic-values}}
                    {{>table}}
                {{/socioeconomic-values}}
            {{/charts}}
        </div>
		  <h2 class="section"><span class="indicator-name"></span> <span>{{#labels}}{{for_countries}}{{/labels}}</span></h2>
		  <div id="chart-region-bar-comparison" class="graph-medium">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#employment-agriculture}}
	                    {{>table}}
	                {{/employment-agriculture}}
            	</div>
            {{/charts}}
          </div>
		  <h2 class="section"><span class="indicator-name"></span> <span>{{#labels}}{{across_time}}{{/labels}} <span class="country-name">{{#info}}{{country}}{{/info}}</span></span></h2>
		  <div class="row">
			<div class="col-xs-4">
				{{#labels}}{{compare_with}}{{/labels}}:
			</div>
			<div class="col-xs-8">
			  <select id="country-comparison" class="form-control">
			  	{{#selectors}}
				  	{{#countries}}
						<option value="{{iso3}}">{{name}}</option>
					{{/countries}}
				{{/selectors}}
			  </select>
			</div>
		  </div>
		  <div id="employment-timeline" class="graph">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#employment-timeline}}
	                    {{>table}}
	                {{/employment-timeline}}
            	</div>
            {{/charts}}          
          </div>
		  <div class="row evolutions">
			<div class="col-xs-2 lead">{{#labels}}{{land}}{{/labels}}</div>
			<div class="col-xs-2"></div>
			<div id="land-1" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#land-1}}
	                    {{>table}}
	                {{/land-1}}
            	</div>
            {{/charts}}
            </div>
			<div id="land-2" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#land-2}}
	                    {{>table}}
	                {{/land-2}}
            	</div>
            {{/charts}}            
            </div>
			<div id="land-3" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#land-3}}
	                    {{>table}}
	                {{/land-3}}
            	</div>
            {{/charts}}            
            </div>
			<div id="land-4" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#land-4}}
	                    {{>table}}
	                {{/land-4}}
            	</div>
            {{/charts}}            
            </div>
		  </div>
		  <div class="row evolutions">
			<div class="col-xs-2 lead">{{#labels}}{{gender}}{{/labels}}</div>
			<div class="col-xs-2"></div>
			<div id="gender-1" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#gender-1}}
	                    {{>table}}
	                {{/gender-1}}
            	</div>
            {{/charts}}            
            </div>
			<div id="gender-2" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#gender-2}}
	                    {{>table}}
	                {{/gender-2}}
            	</div>
            {{/charts}}            
            </div>
			<div id="gender-3" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#gender-3}}
	                    {{>table}}
	                {{/gender-3}}
            	</div>
            {{/charts}}            
            </div>
			<div id="gender-4" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#gender-4}}
	                    {{>table}}
	                {{/gender-4}}
            	</div>
            {{/charts}}            
            </div>
		  </div>
		  <div class="row evolutions">
			<div class="col-xs-2 lead">{{#labels}}{{hunger}}{{/labels}}</div>
			<div class="col-xs-2"></div>
			<div id="hunger-1" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#hunger-1}}
	                    {{>table}}
	                {{/hunger-1}}
            	</div>
            {{/charts}}            
            </div>
			<div id="hunger-2" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#hunger-2}}
	                    {{>table}}
	                {{/hunger-2}}
            	</div>
            {{/charts}}            
            </div>
			<div id="hunger-3" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#hunger-3}}
	                    {{>table}}
	                {{/hunger-3}}
            	</div>
            {{/charts}}            
            </div>
			<div id="hunger-4" class="col-xs-2 graph-small">
            {{#charts}}
            	<div class="source-graph chart-content">
	                {{#hunger-4}}
	                    {{>table}}
	                {{/hunger-4}}
            	</div>
            {{/charts}}            
            </div>
		  </div>
	  </div>
	  <div class="col-sm-3">
		  <h2 class="section"><span>{{#labels}}{{sources}}{{/labels}}</span></h2>
		  <select id="source-select" multiple="multiple" class="data-sources">
		  	{{#selectors}}
			  	{{#data-sources}}
					<option>{{name}}</option>
				{{/data-sources}}
			{{/selectors}}
		  </select>
		  <h2 class="section"><span>{{#labels}}{{indicators}}{{/labels}}</span></h2>
		  <select id="indicator-select" class="form-control">
			  	{{#selectors}}
				  	{{#indicators}}
				  		<optgroup label="{{name}}">
				  			{{#indicators}}
								<option>{{name}}</option>
							{{/indicators}}
				  		</optgroup>
					{{/indicators}}
				{{/selectors}}
		  </select>
		  <button class="btn data-button">{{#labels}}{{full_data}}{{/labels}} <span class="country-name">{{#info}}{{country}}{{/info}}</span></button>
		  <a href="" class="more-info">{{#labels}}{{more_about_the_indicators}}{{/labels}}</a>
		  <h2 class="section"><span>{{#labels}}{{specific_country}}{{/labels}}</span></h2>
		  <select id="country-select" class="form-control">
		  	{{#selectors}}
			  	{{#countries}}
					<option value="{{iso3}}">{{name}}</option>
				{{/countries}}
			{{/selectors}}
		  </select>
		  <a href="" class="more-info">{{#labels}}{{more_about_areas_and_countries}}{{/labels}}</a>
		  <button class="btn data-button">
			{{#labels}}{{more_resources}}{{/labels}} <span class="country-name">{{#info}}{{country}}{{/info}}</span>
			<p class="at-button">{{#labels}}{{at}}{{/labels}} landportal.info</p>
		  </button>
		  <button class="btn data-button-dark">{{#labels}}{{land_debates}}{{/labels}} <span class="country-name">{{#info}}{{country}}{{/info}}</span></button>
	  </div>
	</div>
</div>
{{>footer}}