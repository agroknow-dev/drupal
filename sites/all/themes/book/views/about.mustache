	<p>{{#labels}}{{{about_opendev_description_2}}}{{/labels}}</p>
	<ul>{{#labels}}{{{about_opendev_description_3}}}{{/labels}}</ul>
	<p>{{#labels}}{{{about_opendev_description_4}}}{{/labels}}</p>
	<p>{{#labels}}{{{about_opendev_description_5}}}{{/labels}}</p>
